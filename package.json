{
  "name": "tg-contest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "test": "standard",
    "deploy": "rm -rf dist && parcel build index.html && gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "linters": {
      "{src,test}/**/*.js": [
        "prettier-standard",
        "standard --fix",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.12.2",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1"
  }
}
